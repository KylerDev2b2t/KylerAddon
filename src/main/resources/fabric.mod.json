  "mixins": [
    "kyler-addon.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "25,225,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
