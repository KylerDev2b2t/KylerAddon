{
  "schemaVersion": 1,
  "id": "kyler-addon",
  "version": "${version}",
  "name": "Kyler Addon",
  "description": "A Meteor Client addon by Kyler.",
  "authors": [
    "Kyler"
  ],
  "contact": {
    "repo": "https://github.com/YourUsername/kyler-addon"
  },
  "icon": "assets/kyler/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.kyler.addon.KylerAddon"
    ]
